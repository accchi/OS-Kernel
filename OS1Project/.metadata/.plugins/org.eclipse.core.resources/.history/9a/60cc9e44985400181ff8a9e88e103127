#include <IOSTREAM.h>
#include "List.h"
#include "PCB.h"


int main(){
	List list;
	list.addToEnd(new PCB());
	list.addToEnd(new PCB());
	list.addToEnd(new PCB());
	list.addToEnd(new PCB());
	list.addToEnd(new PCB());
	list.addToEnd(new PCB());
	List::Elem* tmp=list.first;
	while(tmp){
		cout<<tmp->pcb->ID;
		tmp=tmp->next;
	}


	return 0;
}



